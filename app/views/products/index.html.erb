<% @products.each do |f| %>
  <p>
  <%= image_tag "/get_image/#{f.id}" %>
  <%= f.title %>
  <%= f.price %>円
  <%= link_to '削除', product_path(f.id), method: :delete %>
  <%= link_to '表示', product_path(f.id) %>
  </p>
  <% if @flag == true then %>
  <p>
      Sold Out<br>
  </p>
  <% end %>
<% end %>


<p>
    <%= link_to "新商品追加", new_product_path %><br>
</p>


